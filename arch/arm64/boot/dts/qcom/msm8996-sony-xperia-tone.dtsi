	pmic@3 {
		regulators {
			compatible = "qcom,pmi8994-regulators";
			#address-cells = <1>;
			#size-cells = <1>;

			/* GPU GX GDSC Supply */
			pmi8994_s2: s2@1700 {
				reg = <0x1700 0x100>;
				status = "ok";
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1015000>;
				regulator-always-on;
			};
		};
	};
};

/* Pins */
&msmgpio {
	gpio-reserved-ranges = <0 4>;

	blsp2_i2c11_default: blsp2_i2c11 {
		pinmux {
			function = "blsp_i2c11";
			pins = "gpio60", "gpio61";
		};
		pinconf {
			pins = "gpio60", "gpio61";
			drive-strength = <16>;
			bias-disable;
		};
	};

	blsp2_i2c11_sleep: blsp2_i2c11_sleep {
		pinmux {
			function = "gpio";
			pins = "gpio60", "gpio61";
		};
		pinconf {
			pins = "gpio60", "gpio61";
			drive-strength = <2>;
			bias-disable;
		};
	};

