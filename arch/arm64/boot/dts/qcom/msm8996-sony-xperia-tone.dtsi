			drive-strength = <2>;
		};
	};

	usb_detect_weak_gpio: usb_detect_weak {
		pinmux {
			pins = "gpio130";
			function = "gpio";
		};

		pinconf {
			pins = "gpio130";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};

	fp_int_n_gpio: fp_int_n {
		pinmux {
			pins = "gpio132";
			function = "gpio";
		};

		pinconf {
			pins = "gpio132";
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	touch_int_n_gpio_act: touch_int_n_act {
		pinmux {
			pins = "gpio125";
			function = "touch_int_n";
		};
		pinconf {
			pins = "gpio125";
			drive-strength = <2>;
			bias-pull-up;
		};
	};

	touch_int_n_gpio_sleep: touch_int_n_slp {
		pinmux {
			pins = "gpio125";
			function = "touch_int_n";
		};
		pinconf {
			pins = "gpio125";
			drive-strength = <2>;
			bias-pull-down;
		};
	};
};

