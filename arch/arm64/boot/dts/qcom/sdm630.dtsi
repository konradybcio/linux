			pinctrl-0 = <&blsp2_uart1_tx_active &blsp2_uart1_rxcts_active
				&blsp2_uart1_rfr_active>;
			pinctrl-1 = <&blsp2_uart1_tx_sleep &blsp2_uart1_rxcts_sleep
